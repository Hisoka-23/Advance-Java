<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>ConfigContextApp</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	<context-param>
		<param-name>website</param-name>
		<param-value>www.ashokitech.com</param-value>
	</context-param>
	
	<servlet>
		<servlet-name>FirstServlet</servlet-name>
		<servlet-class>in.prakash.servlet.FirstServlet</servlet-class>
		
		<init-param>
			<param-name>msg1</param-name>
			<param-value>Welcome to first servlet</param-value>
		</init-param>
		
	</servlet>
	
	<servlet-mapping>
		<servlet-name>FirstServlet</servlet-name>
		<url-pattern>/first</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SecondServlet</servlet-name>
		<servlet-class>in.prakash.servlet.SecondServlet</servlet-class>
		
		<init-param>
			<param-name>msg2</param-name>
			<param-value>Welcome to first Second</param-value>
		</init-param>
		
	</servlet>
	
	<servlet-mapping>
		<servlet-name>SecondServlet</servlet-name>
		<url-pattern>/second</url-pattern>
	</servlet-mapping>
	
</web-app>
